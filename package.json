{
    "name": "crmserver",
    "description": "",
    "author": "GrapeoffJS",
    "private": true,
    "engines": {
        "node": "16.13.2"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build && nest build admin-panel",
        "build:crm": "nest build",
        "build:admin": "nest build admin-panel",
        "start:dev": "nest start --watch",
        "start:dev:admin": "nest start --watch admin-panel",
        "start:debug": "nest start --debug --watch",
        "start:prod:crm": "node dist/apps/crm/main.js",
        "start:prod:admin": "node dist/apps/admin-panel/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e:crmserver": "jest --config apps/crm/test/jest-e2e.json",
        "doc": "npx @compodoc/compodoc -p tsconfig.json -s"
    },
    "dependencies": {
        "@elastic/elasticsearch": "7.13.0",
        "@nestjs/axios": "^0.0.5",
        "@nestjs/cli": "^8.2.0",
        "@nestjs/common": "^8.2.6",
        "@nestjs/config": "^1.1.6",
        "@nestjs/core": "^8.2.6",
        "@nestjs/elasticsearch": "^8.0.0",
        "@nestjs/event-emitter": "^1.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "^1.0.1",
        "@nestjs/platform-express": "^8.2.6",
        "@nestjs/swagger": "^5.2.0",
        "@nestjs/terminus": "^8.0.4",
        "@nestjs/throttler": "^2.0.0",
        "@typegoose/typegoose": "^9.5.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "0.5.1",
        "class-validator": "^0.13.1",
        "compression": "^1.7.4",
        "csvtojson": "^2.0.10",
        "helmet": "^5.0.2",
        "joi": "^17.6.0",
        "libphonenumber-js": "^1.9.47",
        "luxon": "^2.3.0",
        "mongoose": "^6.1.8",
        "mongoose-autopopulate": "^0.16.0",
        "multer": "^1.4.4",
        "nestjs-typegoose": "^7.1.38",
        "pm2": "^5.1.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.2",
        "swagger-ui-express": "^4.3.0",
        "xlsx": "^0.17.5"
    },
    "devDependencies": {
        "@nestjs/schematics": "^8.0.5",
        "@nestjs/testing": "^8.2.6",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.0",
        "@types/csvtojson": "^2.0.0",
        "@types/express": "^4.17.11",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^27.4.0",
        "@types/joi": "^17.2.3",
        "@types/luxon": "^2.0.9",
        "@types/mongoose": "^5.11.97",
        "@types/mongoose-autopopulate": "^0.10.2",
        "@types/mongoose-lean-virtuals": "^0.5.2",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.13",
        "@types/supertest": "^2.0.10",
        "@types/xlsx": "^0.0.36",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-sonarjs": "^0.11.0",
        "jest": "^27.4.7",
        "prettier": "^2.2.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.5.5",
        "install": "^0.13.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/apps/"
        ]
    }
}