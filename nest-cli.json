{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/crm/src",
  "monorepo": true,
  "root": "apps/crm",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/crm/tsconfig.app.json"
  },
  "projects": {
    "crm": {
      "type": "application",
      "root": "apps/crm",
      "entryFile": "main",
      "sourceRoot": "apps/crm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crm/tsconfig.app.json"
      }
    },
    "admin-panel": {
      "type": "application",
      "root": "apps/admin-panel",
      "entryFile": "main",
      "sourceRoot": "apps/admin-panel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-panel/tsconfig.app.json"
      }
    }
  }
}